{% extends "layout/base.html.jinja2" %}
{% block title %}
職能發展學院-最新消息
{% endblock %}

{% block main %}
<h1>{{ self.title() }}</h1>

{{ rows }}

<table border="1" cellpadding="5" cellspacing="0" style="border-collapse: collapse; width: 100%;">
    <thead style="background-color: #f2f2f2;">
        <tr>
            {% for colname in colnames %}
            <th>{{ colname }}</th>
            {% endfor %}
        </tr>
    </thead>
    <tbody>
        {% for row in rows %}
        <tr>
            {% for col in row %}
            <td>{{ col }}</td>
            {% endfor %}
        </tr>
        {% endfor %}
    </tbody>
</table>

{{ super() }}
{% endblock %}